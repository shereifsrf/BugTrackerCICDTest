    <script type="text/javascript">
        $(document).ready(function () {
            $('#pageContent').css('min-height', $(window).height() - $('footer').outerHeight(true));

            var pageClicked = window.location.pathname;

            $(".nav-item").each(function () {
                $(this).removeClass("active");

                if (pageClicked == $(this).children().attr("href")) {
                    $(this).addClass("active");
                }
            });

            if ($('#icon-bell').length > 0 && !$('#notificationPanel').hasClass('active')) {
                var today = new Date();
                generateItems(today);
            }

            setInterval(function () {
                if ($('#icon-bell').length > 0 && !$('#notificationPanel').hasClass('active')) {
                    var today = new Date();
                    generateItems(today);
                }
            }, 60000);
        });

        function logout() {
            $.ajax({
                type: "POST",
                url: "../Common.asmx/logout",
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    Swal.fire({
                        icon: response.d.isSuccess ? 'success' : error,
                        text: response.d.returnMsg
                    });

                    window.location.href = '/index.aspx';
                },
                error: function (response) {
                    console.log(response);
                },
                failure: function (response) {
                    console.log(response);
                }
            });
        }

        function refreshNotifications(items, today) {

            var cssTransitionEnd = getTransitionEnd();
            var container = $('body');

            /*items.sort(function (a, b) {
                if (a.isExpired === b.isExpired) {
                    return a.date - b.date;
                } else {
                    return (b.isExpired ? 0 : 1) - (a.isExpired ? 0 : 1);
                }
            });*/

            var html = `<div id="notificationPanel" class="notifications js-notifications">
	                        <h3>Notifications</h3>
	                        <ul class="notifications-list">
		                        <li class="item no-data">You don't have notifications</li>`

            for (i = 0; i < items.length; i++) {
                html += `<li class="item js-item" data-id="` + items[i].id + `">
			                <div class="details"><span class="title">`+ items[i].title + `</span><span class="date">` + items[i].formattedDate + `</span></div>
			                <button type="button" class="button-default button-dismiss js-dismiss" onclick="return markNotificationAsRead(`+ items[i].id + `)">×</button>
		                </li>`
            }

            html += `</ul> </div>`;

            container
                .append(html)
                .find('.js-count').attr('data-count', items.length).html(items.length).end()
                .on('click', '.js-show-notifications', function (event) {
                    $(event.currentTarget).closest('.js-show-notifications').toggleClass('active').blur();
                    $('#notificationPanel').toggleClass('active').blur();

                    if ($('#icon-bell').length > 0) {
                        $('.notifications.active').css('top', $('#icon-bell').position().top + 45);
                        $('.notifications.active').css('left', $('#icon-bell').position().left - 315);
                    }

                    return true;
                })
                .on('click', '.js-dismiss', function (event) {
                    var item = $(event.currentTarget).parents('.js-item');

                    var removeItem = function () {
                        item[0].removeEventListener(cssTransitionEnd, removeItem, false);
                        item.remove();

                        /* update notifications' counter */
                        var countElement = container.find('.js-count');
                        var prevCount = +countElement.attr('data-count');
                        var newCount = prevCount - 1;
                        countElement
                            .attr('data-count', newCount)
                            .html(newCount);

                        if (newCount === 0) {
                            countElement.remove();
                            container.find('.js-notifications').addClass('empty');
                        }
                    };

                    item[0].addEventListener(cssTransitionEnd, removeItem, false);
                    item.addClass('dismissed');
                    return true;
                });

            if (items.length > 0) {
                $('.js-count').show();
            } else {
                container.find('.js-notifications').addClass('empty');
            }
        }

        function changeJsonDateTimeToJavaScriptDateTime(jsonDate) {
            var dateString = jsonDate.substr(6);

            var javaScriptTime = new Date(parseInt(dateString));

            return javaScriptTime;
        }

        function generateItems(today) {
            var notiList = [];

            $.ajax({
                type: "POST",
                url: "/Common.asmx/getNotificationList",
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                global: false,
                success: function (response) {
                    var res = response.d;
                    for (i = 0; i < res.length; i++) {
                        var noti = {
                            id: res[i].notificationId, title: res[i].content, date: changeJsonDateTimeToJavaScriptDateTime(res[i].createdOn),
                            formattedDate: changDateTimeFormat(res[i].createdOn), isExpired: (changeJsonDateTimeToJavaScriptDateTime(res[i].createdOn) < today)
                        };

                        notiList.push(noti);
                    }
                    refreshNotifications(notiList, today);
                },
                error: function (response) {
                    console.log(response);
                    showErrorMsg();
                },
                failure: function (response) {
                    console.log(response);
                    showErrorMsg();
                }
            });
        }

        function markNotificationAsRead(notiId) {
            $.ajax({
                type: "POST",
                url: "/Common.asmx/markNotificationAsRead",
                data: JSON.stringify({ 'notificationId': notiId }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    console.log(response)
                },
                error: function (response) {
                    console.log(response);
                    showErrorMsg();
                },
                failure: function (response) {
                    console.log(response);
                    showErrorMsg();
                }
            });

            return false;
        }

        function getTransitionEnd() {
            var supportedStyles = window.document.createElement('fake').style;
            var properties = {
                'webkitTransition': { 'end': 'webkitTransitionEnd' },
                'oTransition': { 'end': 'oTransitionEnd' },
                'msTransition': { 'end': 'msTransitionEnd' },
                'transition': { 'end': 'transitionend' }
            };

            var match = null;
            Object.getOwnPropertyNames(properties).forEach(function (name) {
                if (!match && name in supportedStyles) {
                    match = name;
                    return;
                }
            });

            return (properties[match] || {}).end;
        }
    </script>

